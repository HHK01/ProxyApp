{
  "Endpoints": [
    {
      "Name": "Endpoint1", /* The backend endpoint definition */
      "ProxyUrlPath": "Backend/Endpoint1", /* The service will ping the controller named "BackendController" using this setting the controller path*/
      "BackendSwaggerEndpoint": "https://petstore.swagger.io/v2/swagger.json", /* The backend swagger json schema using the external URL that you want to hide */
      "BackendBaseEndpoint": "https://petstore.swagger.io/v2" 
    },
    {
      "Name": "Endpoint2",
      "ProxyUrlPath": "Backend/Endpoint2",
      "BackendSwaggerEndpoint": "https://petstore.swagger.io/v2/swagger.json",
      "BackendBaseEndpoint": "https://petstore.swagger.io/v2"
    }
  ]
,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
